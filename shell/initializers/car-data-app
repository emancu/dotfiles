if [ -z $CAR_DATA_SETUP ]; then
  echo 'Project setup..'

  source $DOTFILES_PATH/shell/initializers/carwow_common

  export HEROKU_APP=$CARWOW_HEROKU_CAR_DATA_APP-uk
  export HEROKU_APP_DE=$CARWOW_HEROKU_CAR_DATA_APP-de
  export HEROKU_APP_ES=$CARWOW_HEROKU_CAR_DATA_APP-es

  export CAR_DATA_SETUP=true
fi
