if [ -z $QUOTES_SITE_SETUP ]; then
  echo 'Project setup..'

  source $DOTFILES_PATH/shell/initializers/carwow_common

  export HEROKU_APP=$CARWOW_HEROKU_QUOTES_APP-eu
  export HEROKU_APP_DE=$CARWOW_HEROKU_QUOTES_APP-de
  export HEROKU_APP_ES=$CARWOW_HEROKU_QUOTES_APP-es

  export QUOTES_SITE_SETUP=true
fi
